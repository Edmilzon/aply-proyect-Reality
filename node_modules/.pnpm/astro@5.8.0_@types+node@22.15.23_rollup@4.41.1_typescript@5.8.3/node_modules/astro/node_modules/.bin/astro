#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/edmilzon/Document/Proyects/sistemas-economicos/sistemas-ecnomicos-app/awesome-ablation/node_modules/.pnpm/astro@5.8.0_@types+node@22.15.23_rollup@4.41.1_typescript@5.8.3/node_modules/astro/node_modules:/home/edmilzon/Document/Proyects/sistemas-economicos/sistemas-ecnomicos-app/awesome-ablation/node_modules/.pnpm/astro@5.8.0_@types+node@22.15.23_rollup@4.41.1_typescript@5.8.3/node_modules:/home/edmilzon/Document/Proyects/sistemas-economicos/sistemas-ecnomicos-app/awesome-ablation/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/edmilzon/Document/Proyects/sistemas-economicos/sistemas-ecnomicos-app/awesome-ablation/node_modules/.pnpm/astro@5.8.0_@types+node@22.15.23_rollup@4.41.1_typescript@5.8.3/node_modules/astro/node_modules:/home/edmilzon/Document/Proyects/sistemas-economicos/sistemas-ecnomicos-app/awesome-ablation/node_modules/.pnpm/astro@5.8.0_@types+node@22.15.23_rollup@4.41.1_typescript@5.8.3/node_modules:/home/edmilzon/Document/Proyects/sistemas-economicos/sistemas-ecnomicos-app/awesome-ablation/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../astro.js" "$@"
else
  exec node  "$basedir/../../astro.js" "$@"
fi
