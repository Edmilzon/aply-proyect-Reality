#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/d/PROGRAMAS/WEB/Proyect-sistemas-economicos/aply-proyect-Reality/node_modules/.pnpm/vite@6.3.5_@types+node@22.1_054e2b78cf4b5b5d65fb58737b562991/node_modules/vite/bin/node_modules:/proc/cygdrive/d/PROGRAMAS/WEB/Proyect-sistemas-economicos/aply-proyect-Reality/node_modules/.pnpm/vite@6.3.5_@types+node@22.1_054e2b78cf4b5b5d65fb58737b562991/node_modules/vite/node_modules:/proc/cygdrive/d/PROGRAMAS/WEB/Proyect-sistemas-economicos/aply-proyect-Reality/node_modules/.pnpm/vite@6.3.5_@types+node@22.1_054e2b78cf4b5b5d65fb58737b562991/node_modules:/proc/cygdrive/d/PROGRAMAS/WEB/Proyect-sistemas-economicos/aply-proyect-Reality/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/d/PROGRAMAS/WEB/Proyect-sistemas-economicos/aply-proyect-Reality/node_modules/.pnpm/vite@6.3.5_@types+node@22.1_054e2b78cf4b5b5d65fb58737b562991/node_modules/vite/bin/node_modules:/proc/cygdrive/d/PROGRAMAS/WEB/Proyect-sistemas-economicos/aply-proyect-Reality/node_modules/.pnpm/vite@6.3.5_@types+node@22.1_054e2b78cf4b5b5d65fb58737b562991/node_modules/vite/node_modules:/proc/cygdrive/d/PROGRAMAS/WEB/Proyect-sistemas-economicos/aply-proyect-Reality/node_modules/.pnpm/vite@6.3.5_@types+node@22.1_054e2b78cf4b5b5d65fb58737b562991/node_modules:/proc/cygdrive/d/PROGRAMAS/WEB/Proyect-sistemas-economicos/aply-proyect-Reality/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
