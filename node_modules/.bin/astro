#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/d/PROGRAMAS/WEB/Proyect-sistemas-economicos/aply-proyect-Reality/node_modules/.pnpm/astro@5.8.0_@types+node@22._8a6236b30c59639c997ccd91b34d7c71/node_modules/astro/node_modules:/proc/cygdrive/d/PROGRAMAS/WEB/Proyect-sistemas-economicos/aply-proyect-Reality/node_modules/.pnpm/astro@5.8.0_@types+node@22._8a6236b30c59639c997ccd91b34d7c71/node_modules:/proc/cygdrive/d/PROGRAMAS/WEB/Proyect-sistemas-economicos/aply-proyect-Reality/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/d/PROGRAMAS/WEB/Proyect-sistemas-economicos/aply-proyect-Reality/node_modules/.pnpm/astro@5.8.0_@types+node@22._8a6236b30c59639c997ccd91b34d7c71/node_modules/astro/node_modules:/proc/cygdrive/d/PROGRAMAS/WEB/Proyect-sistemas-economicos/aply-proyect-Reality/node_modules/.pnpm/astro@5.8.0_@types+node@22._8a6236b30c59639c997ccd91b34d7c71/node_modules:/proc/cygdrive/d/PROGRAMAS/WEB/Proyect-sistemas-economicos/aply-proyect-Reality/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
